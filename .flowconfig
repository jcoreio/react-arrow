[ignore]
<PROJECT_ROOT>/lib/.*
<PROJECT_ROOT>/node_modules/.*/immutable/.*
<PROJECT_ROOT>/node_modules/.*/html-entities/.*
<PROJECT_ROOT>/node_modules/.*/babel-parse-wild-code/.*
.*/node_modules/.*/tests?/.*\.json
.*/node_modules/fbjs/.*
<PROJECT_ROOT>/dist/.*
.*/malformed_package_json/.*

[include]
./src
./stories
./test

[libs]

[options]
module.system=node
module.name_mapper='.*\.sass$' -> 'empty/object'
module.name_mapper='.*\.scss$' -> 'empty/object'
module.name_mapper='.*\.css$' -> 'empty/object'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
suppress_comment= \\(.\\|\n\\)*\\flow-issue
